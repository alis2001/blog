  <meta name="author" content="Pahlavi for Iran">
  <meta name="robots" content="index, follow">
  <link rel="canonical" href="<%= typeof canonicalUrl !== 'undefined' ? canonicalUrl : 'https://pahlaviforiran.com' %>">
  
  <!-- Favicon -->
  <link rel="icon" type="image/png" href="/images/lion-and-sun.png">
  <link rel="apple-touch-icon" href="/images/lion-and-sun.png">
  <link rel="shortcut icon" type="image/png" href="/images/lion-and-sun.png">
  
  <!-- Open Graph Meta Tags (Facebook, LinkedIn) -->
  <meta property="og:title" content="<%= typeof pageTitle !== 'undefined' ? pageTitle : 'Pahlavi for Iran - News, History & Updates' %>">
  <meta property="og:description" content="<%= typeof metaDescription !== 'undefined' ? metaDescription : 'Latest news and comprehensive history of the Pahlavi dynasty and Iran Revolution 2026.' %>">
  <meta property="og:type" content="<%= typeof ogType !== 'undefined' ? ogType : 'website' %>">
  <meta property="og:url" content="<%= typeof canonicalUrl !== 'undefined' ? canonicalUrl : 'https://pahlaviforiran.com' %>">
  <meta property="og:image" content="<%= typeof ogImage !== 'undefined' ? ogImage : 'https://pahlaviforiran.com/images/pahlavi-banner.png' %>">
