  <meta name="twitter:title" content="<%= typeof pageTitle !== 'undefined' ? pageTitle : 'Pahlavi for Iran - News, History & Updates' %>">
  <meta name="twitter:description" content="<%= typeof metaDescription !== 'undefined' ? metaDescription : 'Latest news and comprehensive history of the Pahlavi dynasty and Iran Revolution 2026.' %>">
  <meta name="twitter:image" content="<%= typeof ogImage !== 'undefined' ? ogImage : 'https://pahlaviforiran.com/images/pahlavi-banner.png' %>">
  
  <link rel="stylesheet" href="/css/public.css?v=<%= Date.now() %>">
  <link rel="stylesheet" href="/css/home.css?v=<%= Date.now() %>">
